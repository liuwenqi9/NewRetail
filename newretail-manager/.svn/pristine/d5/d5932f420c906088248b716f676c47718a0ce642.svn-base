package com.newretail.pojo.custom;
import java.util.List;

/**
 * 这个是通用的返回数据接口
 * @author Yongjie
 *
 * @param <T>
 */
public class GoodsCatResponse<T> {

   private int code;
   private List<T> data;
   private String msg;
   public void setCode(int code) {
        this.code = code;
    }
    public int getCode() {
        return code;
    }

   public void setData(List<T> data) {
        this.data = data;
    }
    public List<T> getData() {
        return data;
    }

   public void setMsg(String msg) {
        this.msg = msg;
    }
    public String getMsg() {
        return msg;
    }
}
